{
  "data": {
    "features_path": "tsocial_processed/node_features.csv",
    "edges_path": "tsocial_processed/edges.csv",
    "labels_path": "tsocial_processed/node_labels.csv",
    "static_save_path": "tsocial_static.pt",
    "embeddings_path": "tsocial_embeddings.npy",
    "standardize_features": true,
    "include_centrality": false,
    "include_motifs": false,
    "include_temporal": false,
    "betweenness_sample_k": 100
  },
  "training": {
    "learning_rate": 0.001,
    "weight_decay": 1e-05,
    "temperature": 0.5,
    "edge_dropout_prob": 0.2,
    "feature_mask_prob": 0.2,
    "gradient_clip_norm": 1.0,
    "save_checkpoints": true,
    "checkpoint_dir": "checkpoints_tsocial",
    "save_every_n_epochs": 10,
    "epochs_pretrain": 5,
    "epochs_finetune": 5,
    "pretrain_batch_size": 256,
    "finetune_batch_size": 512,
    "eval_batch_size": 512,
    "num_neighbors": [
      5,
      5
    ]
  },
  "system": {
    "device": "auto",
    "random_seed": 42,
    "num_workers": 0,
    "log_level": "INFO",
    "log_file": "tsocial_pipeline.log"
  },
  "model": {
    "hidden_channels": 32,
    "latent_dim": 16,
    "projection_dim": 16,
    "dropout": 0.2,
    "num_heads": 1,
    "classifier_hidden": 16
  }
}